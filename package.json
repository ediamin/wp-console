{
  "name": "wp-console",
  "version": "2.5.1",
  "description": "An in-browser PHP console for WordPress powered by PsySH",
  "author": "Edi Amin <to.ediamin@gmail.com>",
  "license": "GPL-3.0",
  "keywords": [
    "wordpress",
    "console",
    "browser",
    "psysh",
    "shell",
    "dump"
  ],
  "homepage": "https://github.com/ediamin/wp-console",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ediamin/wp-console.git"
  },
  "bugs": {
    "url": "https://github.com/ediamin/wp-console/issues"
  },
  "engines": {
    "node": ">=22.20.0",
    "npm": ">=10.9.3"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@wordpress/api-fetch": "7.34.0",
    "@wordpress/components": "30.7.0",
    "@wordpress/compose": "7.34.0",
    "@wordpress/data": "10.34.0",
    "@wordpress/e2e-test-utils": "11.34.0",
    "@wordpress/element": "6.34.0",
    "@wordpress/env": "10.34.0",
    "@wordpress/hooks": "4.34.0",
    "@wordpress/i18n": "6.7.0",
    "@wordpress/scripts": "30.27.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-testing-library": "7.13.3",
    "husky": "9.1.7",
    "jquery": "3.7.1",
    "jsonminify": "0.4.2",
    "lint-staged": "16.2.6",
    "minimist": "1.2.8",
    "nanoid": "5.1.6",
    "semver": "7.7.3",
    "shelljs": "0.10.0"
  },
  "scripts": {
    "env:start": "wp-env start --xdebug=debug",
    "env:stop": "wp-env stop",
    "dev": "wp-scripts start",
    "build": "wp-scripts build",
    "lint:js": "wp-scripts lint-js src/ tests/js/",
    "lint:css": "wp-scripts lint-style src/",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "make-pot": "wp i18n make-pot . languages/wp-console.pot --include=includes,src --exclude=wordpress-core",
    "zip": "node build",
    "release": "npm run make-pot && npm run clean && npm run build && node build",
    "clean": "rimraf assets/css && rimraf assets/js",
    "test:e2e": "wp-scripts test-e2e",
    "test:e2e:debug": "wp-scripts --inspect-brk test-e2e --puppeteer-devtools",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "wp-scripts lint-js"
    ],
    "*.scss": [
      "wp-scripts lint-style"
    ],
    "package.json": [
      "wp-scripts lint-pkg-json"
    ]
  }
}
